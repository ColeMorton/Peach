{
  "name": "@panorama-berlin/antigua",
  "version": "1.0.4",
  "description": "react ui component library",
  "main": "es/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "engines": {
    "npm": ">=3",
    "node": ">= 6.x <= 9.x",
    "yarn": ">=1.3.2 <=1.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "nwb build-react-component --copy-files --no-demo",
    "build:storybook": "build-storybook -c .storybook -o .storybook-dist",
    "clean": "nwb clean-module && nwb clean-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "prepare": "npm run build",
    "format": "prettier-standard 'src/**/*.js'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "./src/**/*.{js,json,css,md}": [
      "yarn run format",
      "git add"
    ]
  },
  "dependencies": {
    "@panorama-berlin/themes": "^1.0.1",
    "npm": "^6.0.0"
  },
  "peerDependencies": {
    "qrious": "4.0.2",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-helmet": "5.1.3",
    "react-intl": "2.3.0",
    "react-modal": "3.1.11",
    "react-qr-reader": "2.1.0",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.79.1",
    "styled-components": "^3.2.5",
    "styled-theme": "^0.3.3",
    "styled-tools": "^0.5.0"
  },
  "devDependencies": {
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^0.0.10",
    "@storybook/addon-notes": "^3.4.1",
    "@storybook/addon-options": "^3.4.1",
    "@storybook/addon-viewport": "^3.4.1",
    "@storybook/react": "^3.4.0",
    "babel-core": "^6.26.0",
    "babel-plugin-styled-components": "^1.5.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.5",
    "nwb": "^0.21.5",
    "prettier-standard": "^8.0.1",
    "qrious": "4.0.2",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-helmet": "5.1.3",
    "react-intl": "2.3.0",
    "react-modal": "3.1.11",
    "react-qr-reader": "2.1.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.79.1",
    "styled-components": "^3.2.5",
    "styled-theme": "^0.3.3",
    "styled-tools": "^0.5.0"
  },
  "author": "Panorama Berlin",
  "homepage": "",
  "license": "GPL-3.0",
  "repository": "https://github.com/panorama-berlin/Peach/tree/master/packages/antigua",
  "keywords": [
    "panorama-berlin"
  ]
}
